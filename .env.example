# Lighter DEX Configuration
# Copy this to .env and fill in your values

# Your Ethereum private key (for signing transactions)
ETH_PRIVATE_KEY=your_eth_private_key_here

# API Key private key (create one on Lighter)
API_KEY_PRIVATE_KEY=your_api_key_private_key_here

# API Key index (3-254, pick any unused one)
API_KEY_INDEX=3

# Your account index on Lighter
ACCOUNT_INDEX=1

# Network: mainnet or testnet
NETWORK=mainnet

# Trading Configuration
MAX_POSITION_USD=5.0
MAX_LOSS_USD=1.0
